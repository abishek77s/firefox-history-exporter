{
  "manifest_version": 2,
  "name": "History Exporter by Web Wrapped",
  "version": "1.0",
  "description": "Export browsing history CSV format.",
  "permissions": [
    "history",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "icons/ww.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "history-exporter@webwrapped.com",
      "strict_min_version": "58.0"
    }
  }
}
